<html>
    <head>
        <!--Bootsrap4 libary.-->
        <link rel="stylesheet" href="lib/css/bootstrap.css">
    </head>
    <body>
        <div class="jumbotron">
            <h1 class="display-4">Sport Compass</h1>
            <p class="lead">All Images on DB and Uploads Folder!</p>
          </div>

          

        <div class="container">   
            <div class="row">
    <% rows.forEach(function(items){ %>
                <div class="col-lg-4">
                 
                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" style=" height: 320px;" src="Uploads/images/<%=items.images_name%>" alt="Card image cap">
                        <div class="card-body">
                          <h5 class="card-title"><%=items.images_name%></h5>
                          <p class="card-text"><%=items.images_mimetype%></p>
                          <a href="/" class="btn btn-info">Home</a>
                        </div>
                      </div>
                     
                </div>
             <% }) %></div>
        </div>
        
    </body>
</html>